category: n/a
prep_time: 15
cook_time: 15
num_servings: 8
ingredients:
	name: lg. chicken breast (3/4 lb)*
	amount: 1
	unit: n/a
	name: pineapple chunks in juice
	amount: 15 oz
	unit: can
	name: black beans
	amount: 15 oz
	unit: can
	name: whole green onions
	amount: 3
