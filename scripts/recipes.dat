cook_time: 30
num_servings: 20
ingredients:
	name: large eggs, whites only
	amount: 2
	unit: n/a
	name: granulated sugar 
	amount: 1/2
	unit: cup
	name: cream of tartar 
	amount: 1/8
	unit: tsp
	name: of salt 
	amount: n/a
	unit: pinch
	name: vanilla extract 
	amount: 1/4
	unit: tsp
	name: slivered almonds
	amount: 2/3
