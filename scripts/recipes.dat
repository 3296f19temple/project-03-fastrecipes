category: n/a
prep_time: 45
cook_time: 10
num_servings: 4
ingredients:
	name: extra firm tofu
	amount: 14
	unit: oz.
	name: salt
	amount: 1/4
	unit: tsp
	name: cornstarch
	amount: 2
	unit: Tbsp
	name: cooking oil
	amount: 2
	unit: Tbsp
	name: hoisin sauce
	amount: 1/2
	unit: cup
	name: green onions
	amount: 2
	unit: n/a
	name: carrot
	amount: 1
