name: Easy Cheesy Broccoli Rice
url: https://www.budgetbytes.com/
img_url: https://www.budgetbytes.com/wp-content/uploads/2017/09/Easy-Cheesy-Broccoli-Rice-pot-200x200.jpg
category: n/a
prep_time: 10
cook_time: 20
num_servings: 6
ingredients:
	name: frozen broccoli florets, thawed
	amount: 1
	unit: lb.
	name: long grain rice
	amount: 2 
	unit: cups
	name: butter
	amount: 2
	unit: Tbsp
	name: salt
	amount: 1/2
	unit: tsp
	name: garlic powder
	amount: 1/4
	unit: tsp
	name: cayenne pepper (optional)
	amount: 1/8
