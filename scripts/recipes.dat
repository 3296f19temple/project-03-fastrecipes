name: Crunchy Cranberry Sauce
url: https://www.budgetbytes.com/how-to-save-money-on-produce/
img_url: https://www.budgetbytes.com/wp-content/uploads/2014/11/Crunchy-Cranberry-Sauce-cups-1-200x200.jpg
category: n/a
prep_time: 2
cook_time: 10
num_servings: 8
ingredients:
	name: granulated sugar 
	amount: 1/2
	unit: cup
	name: water 
	amount: 1
	unit: cup
	name: 12 oz bag fresh cranberries 
	amount: 1
	unit: n/a
	name: lemon juice 
	amount: 1/2
	unit: Tbsp
	name: cinnamon 
	amount: 1/4
	unit: tsp
	name: envelope plain gelatin 
	amount: 1 1/4 
	unit: oz.
	name: stalks celery 
	amount: 4
	unit: n/a
	name: medium apple 
	amount: 1
	unit: n/a
	name: walnut pieces 
	amount: 1/2
