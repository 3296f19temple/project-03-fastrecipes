ingredients:
	name: sweet potato
	amount: 1
	unit: lb.
	name: all-purpose flour, divided
	amount: 2.25
	unit: cups
	name: baking powder
	amount: 1
	unit: Tbsp
	name: sugar
	amount: 1
	unit: Tbsp
	name: salt
	amount: 3/4
