num_servings: 6
ingredients:
	name: large eggs 
	amount: 6
	unit: n/a
	name: milk 
	amount: 1/4
	unit: cup
	name: salsa 
	amount: 1
	unit: cup
	name: shredded cheddar cheese 
	amount: 1/2
	unit: cup
	name: thick tortrilla strips 
	amount: 5
	unit: oz
	name: bunch cilantro, chopped
	amount: 1/4
