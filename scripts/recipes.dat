url: https://www.budgetbytes.com/45-budget-friendly-low-carb-recipes/
img_url: https://www.budgetbytes.com/wp-content/uploads/2017/01/Chicken-and-Cranberry-Salad-with-Lemon-Poppy-Seed-Dressing-close-200x200.jpg
category: n/a
prep_time: 30
cook_time: -1
num_servings: 4
ingredients:
	name: neutral salad oil* 
	amount: 1/4
	unit: cup
	name: fresh lemon juice 
	amount: 2
	unit: Tbsp
	name: honey 
	amount: 1/2
	unit: Tbsp
	name: sugar 
	amount: 1/4
	unit: tsp
	name: Dijon mustard 
	amount: 1/2
	unit: Tbsp
	name: poppy seeds 
	amount: 1/4
	unit: Tbsp
	name: onion powder 
	amount: 1/8
	unit: tsp
	name: salt 
	amount: 1/4
	unit: tsp
	name: shredded cabbage, (about 1/2 head)
	amount: 6
	unit: cups
	name: Salt, as needed
	amount: n/a
	unit: n/a
	name: cooked chicken, diced or shredded
	amount: 1
	unit: cup
	name: sliced almonds 
	amount: 1/2
	unit: cup
	name: dried cranberries
	amount: 1/2
	unit: cup
	name: green onions, sliced
	amount: 3
