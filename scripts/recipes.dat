cook_time: 25
num_servings: 4
ingredients:
	name: corn tortillas
	amount: 6
	unit: n/a
	name: cooking oil 
	amount: 1/2
	unit: Tbsp
	name: butter
	amount: 2
	unit: Tbsp
	name: eggs
	amount: 8
	unit: large
