category: n/a
prep_time: 30
cook_time: 45
num_servings: 4
ingredients:
	name: medium sweet potato
	amount: 1
	unit: n/a
	name: medium red onion 
	amount: 1
	unit: n/a
	name: olive oil, divided
	amount: 1/4
