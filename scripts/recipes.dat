name: Caramelized Onion and Artichoke Flatbread
url: https://www.budgetbytes.com/how-to-save-money-on-produce/
img_url: https://www.budgetbytes.com/wp-content/uploads/2015/02/Caramelized-Onion-and-Artichoke-Flat-Bread-above-1-200x200.jpg
category: n/a
prep_time: 1
cook_time: 1
num_servings: 3
ingredients:
	name: flour 
	amount: 2
	unit: cups
	name: salt
	amount: 1
	unit: tsp
	name: instant yeast 
	amount: 1/4
	unit: tsp
	name: water 
	amount: 3/4-1
	unit: cup
	name: yellow onions 
	amount: 3
	unit: n/a
	name: butter 
	amount: 2
	unit: Tbsp
	name: olive oil 
	amount: 2
	unit: Tbsp
	name: Italian seasoning blend 
	amount: 1/2
