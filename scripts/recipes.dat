	amount: 2
	unit: Tbsp
	name: toasted sesame oil
	amount: 1
	unit: Tbsp
	name: brown sugar
	amount: 2
	unit: Tbsp
	name: rice vinegar
	amount: 2
	unit: Tbsp
	name: grated fresh ginger
	amount: 1
	unit: Tbsp
	name: garlic, minced
	amount: 2
	unit: cloves
	name: sesame seeds
	amount: 2
