cook_time: 30
num_servings: 2
ingredients:
	name: butter 
	amount: 4
	unit: Tbsp
	name: granulated sugar 
	amount: 1/2
	unit: cup
	name: vanilla 
	amount: 1/4
	unit: tsp
	name: large egg 
