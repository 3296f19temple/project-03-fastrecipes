name: Orange Cranberry Muffins
url: https://www.budgetbytes.com/20-recipes-for-leftover-cabbage/
img_url: https://www.budgetbytes.com/wp-content/uploads/2017/11/Orange-Cranberry-Muffins-lineup-200x200.jpg
category: n/a
prep_time: 20
cook_time: 40
num_servings: 12
ingredients:
	name: large orange, zested and juiced
	amount: 1
	unit: n/a
	name: brown sugar
	amount: 1.5
	unit: Tbsp
	name: white sugar
	amount: 1.5 
	unit: Tbsp
	name: salt
	amount: 1/8
	unit: tsp
	name: butter, melted
	amount: 2
	unit: Tbsp
	name: all-purpose flour
	amount: 6
	unit: Tbsp
	name: flour
	amount: 2
	unit: cups
	name: white sugar
	amount: 1
	unit: cup
