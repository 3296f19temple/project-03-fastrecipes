cook_time: -1
num_servings: 6
ingredients:
	name: mayonnaise
	amount: 1/2
	unit: cup
	name: fresh lime juice (about 1/2 lime)
	amount: 2
	unit: Tbsp
	name: ground cumin
	amount: 1
