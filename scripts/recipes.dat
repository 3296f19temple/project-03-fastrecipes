name: Southwest Steak Bowls
url: https://www.budgetbytes.com/faq/
img_url: https://www.budgetbytes.com/wp-content/uploads/2013/08/Southwest-Steak-Bowls-close-1-200x200.jpg
category: n/a
prep_time: 20
cook_time: 45
num_servings: 4
ingredients:
	name: flank or skirt steak
	amount: 3/4 - 1
	unit: lb
	name: olive oil
	amount: 1 1/2
	unit: Tbsp
	name: garlic
	amount: 1
	unit: clove
	name: ground cumin
	amount: 1/2
	unit: tsp
	name: salt
	amount: 1/4
	unit: tsp
	name: medium lime
	amount: 1
	unit: n/a
	name: uncooked brown rice
